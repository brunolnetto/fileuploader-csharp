{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UploadOptions": {
    "MaxRetryAttempts": 3,
    "InitialRetryDelay": "00:00:01",
    "MaxConcurrency": 5
  },
  "FileUploadSettings": {
    "AllowedFileTypes": [ "image/jpeg", "image/png", "application/pdf" ],
    "MaxFileSizeInBytes": 10485760
  }
}
